{
  "name": "simple-dijs",
  "version": "1.5.0",
  "description": "Simple Javascript Dependency Injection Container (DI) like Pimple",
  "main": "dist/di.js",
  "directories": {
    "test": "test"
  },
  "bugs": {
    "url": "https://github.com/avighier/simple-dijs/issues?q=is%3Aopen+is%3Aissue+label%3Abug"
  },
  "homepage": "https://avighier.github.io/simple-dijs",
  "repository": {
    "type": "git",
    "url": "git://github.com/avighier/simple-dijs.git"
  },
  "scripts": {
    "test": "node_modules/.bin/gulp test",
    "browser-test": "bash -c 'node_modules/.bin/browserify test/di.js -o test/browser/di.js' && bash -c 'node_modules/.bin/mocha-phantomjs test/browser/browser.html' && rm test/browser/di.js",
    "doc": "bash -c 'node_modules/.bin/jsdoc src/di.js -t node_modules/docdash/ -d doc'",
    "build-dist": "node_modules/.bin/gulp build-dist",
    "jshint": "node_modules/.bin/gulp lint",
    "build": "npm run build-dist && npm run jshint && npm run test",
    "prepublish": "npm run build"
  },
  "keywords": [
    "dependency",
    "injection",
    "DI",
    "dependency injection",
    "dependency-injection",
    "pimple",
    "container"
  ],
  "author": "Aubin VIGHIER",
  "license": "ISC",
  "devDependencies": {
    "bower": "^1.7.9",
    "browserify": "^13.0.1",
    "del": "^2.2.1",
    "docdash": "^0.4.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-standard": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.0",
    "gulp-istanbul": "^1.0.0",
    "gulp-mocha": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.4",
    "istanbul": "^0.3.5",
    "jsdoc": "^3.4.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.1.0",
    "mocha-phantomjs": "^4.1.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
